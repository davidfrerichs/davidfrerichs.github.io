# ---------------------------------------
PROTO TitleScreenP [
]
{ 
	DEF TitleScreen Transform {
		children [
			Viewpoint {
				position 0 0 3
			}
		   Transform {
			  translation 0 0 -2
			  children [
				 Shape {
					appearance Appearance {
					   texture ImageTexture {
						  url "Dilbert_tex.gif"
					   } 
					}
					geometry 
					Box {
					size 5 4 1}
				 }
				]
		   }
		 ]
	}
}

# ---------------------------------------
PROTO GRP [
		   exposedField MFNode Children []
]
{
   Transform {
	  children IS Children 
   }
}
# ---------------------------------------

PROTO URL_LOAD [
		field MFString source "sphere.wrl"
		eventOut SFInt32 ready 
		
	]
{
   DEF Root GRP {
	  Children [
		 TitleScreenP {}
		]
   }
   DEF SC Script {
	 directOutput TRUE 
	  eventOut SFInt32 ready IS ready
	  eventIn MFNode node_in 
	  field MFString fetchURL IS source
	  field SFNode GeomRoot  USE Root
	  field SFNode Self USE SC

	  url "vrmlscript:
	function initialize(){
//	Browser.setDescription('START');
	Browser.createVrmlFromURL(fetchURL,Self,'node_in');
}

function node_in(newChildren){
//	GeomRoot.aChildren = newChildren ;
	Browser.replaceWorld(newChildren);
	ready = 1;
	
}
"
   }
}

